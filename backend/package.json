{
  "name": "credit-score-api",
  "version": "1.0.0",
  "description": "Backend API for Credit Score Dashboard",
  "main": "index.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "resolutions": {
    "@types/node": "^20.11.0"
  },
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix ../frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "eslint . --ext .js,.jsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "seed": "node scripts/seed.js",
    "seed:reset": "node scripts/seed.js --reset",
    "docs": "swagger-autogen",
    "init-admin": "node scripts/init-admin.js"
  },
  "keywords": [
    "credit-score",
    "dashboard",
    "api"
  ],
  "dependencies": {
    "@godaddy/terminus": "^4.12.1",
    "@types/mongoose": "^5.11.97",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "compression": "^1.7.4",
    "connect-mongo": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.0.3",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^7.0.1",
    "helmet": "^6.0.1",
    "hpp": "^0.2.3",
    "html-to-text": "^9.0.5",
    "http-status-codes": "^2.2.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.30.1",
    "mongodb": "^5.9.2",
    "mongoose": "^7.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.15",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "openai": "^5.5.1",
    "p-map": "^7.0.3",
    "papaparse": "^5.5.3",
    "pug": "^3.0.2",
    "qrcode": "^1.5.3",
    "rate-limiter-flexible": "^7.1.1",
    "socket.io": "^4.8.1",
    "speakeasy": "^2.0.0",
    "uuid": "^8.3.2",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "xlsx": "^0.18.5",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.7",
    "@types/node": "^20.11.0",
    "@types/nodemailer": "^6.4.17",
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
